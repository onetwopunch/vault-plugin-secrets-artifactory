[
  {
    "name": "docker-test",
    "repo": {
      "include-patterns": ["/mydocker2/**"] ,
      "exclude-patterns": [""],
      "repositories": ["docker-test-west-local", "docker-test-east-local"],
      "actions": {
            "groups" : {
              "VAULT_PLUGIN_OWN_ROLE" : ["read", "write", "annotate"]
            }
      }
    }
  },
  {
    "name": "npm-test",
    "repo": {
      "include-patterns": ["**"] ,
      "exclude-patterns": [""],
      "repositories": ["npm-test-west-local", "npm-test-east-local"],
      "actions": {
            "groups" : {
              "VAULT_PLUGIN_OWN_ROLE" : ["read"]
            }
      }
    }
  }
]

